SET SCAN OFF;

DROP TABLE STIRI CASCADE CONSTRAINTS;
DROP TABLE CATEGORII CASCADE CONSTRAINTS;
DROP TABLE UTILIZATORI CASCADE CONSTRAINTS;
DROP TABLE PREFERINTE CASCADE CONSTRAINTS;
DROP TABLE AUTORI CASCADE CONSTRAINTS;
DROP TABLE CONEXIUNE_AUTORI_STIRI CASCADE CONSTRAINTS;
DROP TABLE COMENTARII CASCADE CONSTRAINTS;
DROP TABLE CONEXIUNE_STIRI_MULTIMEDIA CASCADE CONSTRAINTS;
DROP TABLE MULTIMEDIA CASCADE CONSTRAINTS;


DROP SEQUENCE incr_id_categorie;
DROP SEQUENCE incr_id_utilizator; 
DROP SEQUENCE incr_id_stire;
DROP SEQUENCE incr_id_multimedia; 
DROP SEQUENCE incr_id_comentariu; 
DROP SEQUENCE incr_id_autor;

CREATE SEQUENCE incr_id_categorie START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE incr_id_utilizator START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE incr_id_stire START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE incr_id_multimedia START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE incr_id_comentariu START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE incr_id_autor START WITH 1 INCREMENT BY 1;

 


CREATE TABLE STIRI(
ID_STIRE NUMBER(10) CONSTRAINT ID_STIRE_PK PRIMARY KEY,
NUME_STIRE VARCHAR2(280),
CONTINUT_STIRE VARCHAR2(4000),
DATA_POSTARII DATE,
STARE_STIRE NUMBER(1) DEFAULT 0,
ID_CATEGORIE NUMBER(3)

);

CREATE TABLE CATEGORII(
ID_CATEGORIE NUMBER(3) CONSTRAINT ID_CATEGORIE_PK PRIMARY KEY,
NUME_CATEGORIE  VARCHAR(40),
STARE_CATEGORIE NUMBER(1)
);


CREATE TABLE UTILIZATORI(
ID_UTILIZATOR NUMBER(10) CONSTRAINT ID_UTILIZATOR_PK PRIMARY KEY, 
NUME VARCHAR(80),
PRENUME VARCHAR(70),
EMAIL VARCHAR(50) UNIQUE,
PAROLA VARCHAR(50) UNIQUE,
STARE NUMBER(1)
);


CREATE TABLE PREFERINTE(
ID_UTILIZATOR NUMBER(3),
ID_CATEGORIE NUMBER(3),
CONSTRAINT ID_UTILIZATOR_FK1 FOREIGN KEY (ID_UTILIZATOR) REFERENCES UTILIZATORI(ID_UTILIZATOR),
CONSTRAINT ID_CATEGORIE_FK1 FOREIGN KEY (ID_CATEGORIE) REFERENCES CATEGORII(ID_CATEGORIE),
CONSTRAINT UTILIZATOR_CATEGORIE_FK PRIMARY KEY (ID_UTILIZATOR,ID_CATEGORIE)
);


CREATE TABLE AUTORI(
ID_AUTOR NUMBER(3) CONSTRAINT ID_AUTOR_PK PRIMARY KEY,
NUME VARCHAR2(80),
PRENUME VARCHAR(70),
EMAIL VARCHAR(50) UNIQUE,
PAROLA VARCHAR(50) UNIQUE,
DATA_INREGISTRARII DATE,
STARE_AUTOR NUMBER(1) DEFAULT 1
);

CREATE TABLE CONEXIUNE_AUTORI_STIRI(
ID_AUTOR NUMBER(3),
ID_STIRE NUMBER(10),
CONSTRAINT ID_AUTOR_FK FOREIGN KEY(ID_AUTOR) REFERENCES AUTORI(ID_AUTOR),
CONSTRAINT ID_STIRE_FK FOREIGN KEY (ID_STIRE) REFERENCES STIRI(ID_STIRE),
CONSTRAINT AUTOR_STIRE_PK PRIMARY KEY(ID_AUTOR,ID_STIRE)
);

CREATE TABLE COMENTARII(
ID_COMENTARIU NUMBER(20)  CONSTRAINT ID_COMENTARIU_PK PRIMARY KEY,
CONTINUT VARCHAR(400),
DATA_COMENTARIU DATE DEFAULT CURRENT_DATE,
STARE_COMENTARIU  NUMBER(1) DEFAULT 1,
ID_UTILIZATOR NUMBER(10), CONSTRAINT ID_UTILIZATOR_FK FOREIGN KEY(ID_UTILIZATOR) REFERENCES UTILIZATORI(ID_UTILIZATOR),
ID_STIRE NUMBER(10), CONSTRAINT FK_ID_STIRE FOREIGN KEY (ID_STIRE) REFERENCES STIRI(ID_STIRE)
);

CREATE TABLE MULTIMEDIA(
ID_MULTIMEDIA NUMBER(12)  CONSTRAINT ID_MULTIMEDIA_PK PRIMARY KEY,
URL VARCHAR(300),
TIP_MULTIMEDIA VARCHAR(10),
VIZIBILITATE NUMBER(1)

);

CREATE TABLE CONEXIUNE_STIRI_MULTIMEDIA(
ID_STIRE NUMBER(10) NOT NULL,
CONSTRAINT CONEXIUNE_FK_ID_STIRE FOREIGN KEY (ID_STIRE) REFERENCES STIRI(ID_STIRE),
ID_MULTIMEDIA NUMBER(12), CONSTRAINT ID_MULTIMEDIA_FK FOREIGN KEY(ID_MULTIMEDIA) REFERENCES MULTIMEDIA(ID_MULTIMEDIA)

);

ALTER TABLE STIRI
ADD CONSTRAINT STIRI_CATEGORII_FK FOREIGN KEY(ID_CATEGORIE) REFERENCES CATEGORII(ID_CATEGORIE);







